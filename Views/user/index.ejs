<!-- 
     TRENDING PRODUCTS START  -->
<div class="container-fluid pt-5">
    <div class="text-center mb-4">
        <h2 class="section-title px-5"><span class="px-2">Trending Products</span></h2>
    </div>
    <div class="row px-xl-5 pb-3">
        <!---------------------------------------------------------->

        <% for(var i=0;i<productDetails.length;i++) { %>
            <% if(productDetails[i].addToTrendingProduct==1) { %>

                <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
                    <div class="card product-item border-0 mb-4">
                        <div
                            class="card-header product-img position-relative overflow-hidden bg-transparent border p-0 w-100 h-100">
                            <img class="img-fluid w-100 h-100" src="<%=productDetails[i].picture %>"
                                alt="image not found">
                        </div>
                        <div class="card-body border-left border-right text-center p-0 pt-4 pb-3">
                            <h6 class="text-truncate mb-3">
                                <%= productDetails[i].productName %>
                            </h6>
                            <div class="d-flex justify-content-center">
                                <h6>₹<%= productDetails[i].sellingPrice %>
                                </h6>
                                <h6 class="text-muted ml-2"><del>₹<%= productDetails[i].actualPrice %></del></h6>
                            </div>
                        </div>
                        <div class="card-footer d-flex justify-content-between bg-light border">
                            <a href="viewProductDetailsPage?id=<%= productDetails[i]._id %>"
                                class="btn btn-sm text-dark p-0"><i class="fas fa-eye text-primary mr-1"></i>View
                                Detail</a>
                            <a onclick="addToCart('<%= productDetails[i]._id %>')" class="btn btn-sm text-dark p-0"><i
                                    class="fas fa-shopping-cart text-primary mr-1"></i>Add To Cart</a>
                        </div>
                    </div>
                </div>
                <% } %>
                    <% } %>


                        <!----------------------------------------------------------------->

                        <!-- TRENDING PRODUCTS END  -->

