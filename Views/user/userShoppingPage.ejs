  
            <!-- Shop Product Start -->
          
            <div class="col-lg-9 col-md-12">
                <div class="row pb-3">
                    <% for(var i=0;i<product.length;i++) { %>
                    <div class="col-lg-4 col-md-6 col-sm-12 pb-1">
                        <div class="card product-item border-0 mb-4">
                            <div class="card-header product-img position-relative overflow-hidden bg-transparent border p-0">
                                <img class="img-fluid cat-img " src="<%=product[i].picture %>" alt="image not found">
                            </div>
                            <div class="card-body border-left border-right text-center p-0 pt-4 pb-3">
                                <h6 class="text-truncate mb-3"><%= product[i].productName %></h6>
                                <div class="d-flex justify-content-center">
                                    <h6>₹<%= product[i].sellingPrice %></h6><h6 class="text-muted ml-2"><del>₹<%= product[i].actualPrice %></del></h6>
                                </div>
                            </div>
                            <div class="card-footer d-flex justify-content-between bg-light border">
                                <button href="viewProductDetails?id=<%= product[i]._id %>" class="btn btn-sm text-dark p-0"><i class="fas fa-eye text-primary mr-1"></i>View Detail</button>
                                <button onclick="addToCart('<%= product._id %>')" class="btn btn-sm px-3 ms-3"><i class="fas fa-shopping-cart mr-1"></i> Add To Cart</button>
                              
                            </div>
                        </div>
                    </div>
                    <% } %>
                    
                    
                </div>
            </div>
            <!-- Shop Product End -->
        </div>
    </div>
    <!-- Shop End -->



    <!-- Back to Top -->
    <a href="#" class="btn btn-primary back-to-top"><i class="fa fa-angle-double-up"></i></a>


